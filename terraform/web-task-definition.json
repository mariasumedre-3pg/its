[{
    "cpu": "${cpu}",
    "entryPoint": [
        "/bin/bash",
        "-c",
        "exec ./scripts/docker/server/run-server.sh"
    ],
    "essential": true,
    "image": "${image_repo}",
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "${log_group_name}",
            "awslogs-region": "${log_group_region}",
            "awslogs-stream-prefix": "ecs"
        }
    },
    "environment": [{
        "name": "PARAMETER_PATH",
        "value": "${parameter_path}"
    }],
    "memory": "${memory}",
    "name": "web",
    "portMappings": [{
        "containerPort": 5000
    }]
}]
